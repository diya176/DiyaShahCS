#include <iostream>
#include <cstring>
#include "media.h"

using namespace std;

Media::Media(){
  title=new char [80];
  year=0;
}

Media::Media(char* newtitle, int newyear){
  title=new char[80];
  strcpy(title,newtitle);
  year=newyear;
}


char*Media::getTitle(){
  return title;
}

int Media::getYear(){
  return year;
}


void Media::printMedia() {
  cout << "Title: " << title << endl;
  cout << "Year: " << year << endl;
}
