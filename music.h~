#ifndef MUSIC
#define MUSIC

#include <iostream>
#include <cstring>
#include "media.h"

using namespace std;

class Music: public Media{
 public:
  Music();
  //for artist
  char*artist;
  char*publisher;
  char*title;
  int year;
  int duration;
  Music(char*title,char*artist,char*publisher,int year,int duration);
  ~Music();
  char*getArtist();
  char*getPublisher();
  int getDuration();
  virtual void printMedia();
};

#endif
