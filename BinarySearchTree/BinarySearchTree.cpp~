using namespace std;


struct treeNode {
    int data;
    treeNode* right;
    treeNode* left;
};

void add (treeNode* root, int numtoadd);
void organize (treeNode* root, treeNode* toadd);

int main(){


}


void add(treeNode* root, int numtoadd){
  treeNode* toadd=new treeNode();
  treeNode->data=numtoadd;
  treeNode->right=NULL;
  treeNode->left=NULL;
  organize(root, toadd);
}


